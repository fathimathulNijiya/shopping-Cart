<section>
    <div class="container">
        <div class="row mt-5">
            <a href="/admin/addproducts"  class="btn btn-success ml-auto">Add product</a>
        </div>
        <table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Title</th>
      <th scope="col">category</th>
      <th scope="col">price</th>
     <th >image</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      <th scope="row">1</th>
       <td>{{this.name}}</td>
      <td>{{this.company}}</td>
      <td>{{this.rate}}</td>
      <td><img style="width: 50px;height:50px" src="{{this.image}}" alt="" class=""></td>
    </tr>
    {{/each}}
  </tbody>
</table>
    </div>
</section>